@model System.Collections.Generic.List<WebApplication2.Data.Domains.AccessList>

@{
    Layout = "_AuthLayout";
    foreach (var controller in Model)
    {
        <strong>@controller.Controller</strong>
        <br/>

        foreach (var action in controller.Actions)
        {
            <input type="checkbox" name="@action" />@action
            <br/>
        }
        <br/>
        <br/>
        <br/>
        <br/>
    }
}
